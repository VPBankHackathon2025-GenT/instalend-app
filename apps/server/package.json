{
  "name": "@instalend/server",
  "version": "1.0.0",
  "description": "Backend server for InstaLend application",
  "type": "commonjs",
  "contributors": [
    {
      "name": "Quoc Hung",
      "email": "truongquochung312@gmail.com"
    },
    {
      "name": "Duy Tran",
      "email": "nhatduy240203@gmail.com"
    }
  ],
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "run -T prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest build && nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "run -T eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "run -T jest --config ./jest.config.unit.js",
    "test:update": "run -T jest --config ./jest.config.unit.js -u",
    "test:watch": "run test --watch",
    "test:cov": "run test --coverage",
    "test:ci": "bash ./scripts/unit-test-ci.sh",
    "test:integration": "bash ./scripts/integration-test.sh",
    "test:integration:update": "bash ./scripts/integration-test.sh -u",
    "test:integration:ci": "CI=true bash ./scripts/integration-test.sh",
    "test:e2e": "run -T jest --config ./jest.config.e2e.js"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^11.2.0",
    "compression": "^1.8.0",
    "dotenv": "^16.5.0",
    "helmet": "^8.1.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "zod": "^3.25.63"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.7.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/compression": "^1.8.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
